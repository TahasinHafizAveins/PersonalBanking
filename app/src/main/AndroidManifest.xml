<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iotsoftbd.personalbanking">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".accounceSection.home.AccounceHomeActivity"></activity>
        <activity android:name=".accounceSection.CashoutActivity" />
        <activity android:name=".accounceSection.AddBalanceActivity" />
        <activity android:name=".accounceSection.mainBalance.MainBalenceActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".userAuth.userogin.LoginActivity" />
        <activity android:name=".userAuth.userReg.RegistrationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>